{
  "name": "esday-demo-angular",
  "version": "0.4.1",
  "description": "Minimal demo for using the esday library in a plain html page",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BePo65/esday-demo-angular.git"
  },
  "keywords": [
    "esday",
    "demo",
    "angular",
    "date",
    "time",
    "immutable",
    "moment",
    "dayjs",
    "format"
  ],
  "author": "BePo65",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BePo65/esday-demo-angular/issues"
  },
  "homepage": "https://github.com/BePo65/esday-demo-angular#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:watch": "ng build --watch --configuration development",
    "build:ghpages": "npm run build:prod -- --base-href=/esday-demo-angular/ && del-cli ghpages && cpy \"dist/esday-demo-angular/browser/**\" ghpages  && cpy \"dist/esday-demo-angular/3rdpartylicenses.txt\" ghpages --flat",
    "test": "ng test"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": false,
  "dependencies": {
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/router": "^20.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.5",
    "@angular/cli": "^20.1.5",
    "@angular/compiler-cli": "^20.1.0",
    "@types/jasmine": "~5.1.0",
    "cpy-cli": "^5.0.0",
    "del-cli": "^6.0.0",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2"
  }
}
